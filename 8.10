Phrase = input()

OriginalPhraseForward = ""
OriginalPhraseBackwards = ""

for i in range(len(Phrase)):

   if Phrase[i].isalpha():

       OriginalPhraseForward += Phrase[i].lower()

       OriginalPhraseBackwards = Phrase[i].lower() + OriginalPhraseBackwards


if OriginalPhraseForward == OriginalPhraseBackwards:
    print(Phrase + " is a palindrome")
else:
    print(Phrase + " is not a palindrome")
